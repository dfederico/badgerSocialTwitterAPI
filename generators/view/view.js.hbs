/**
 * This file is meant to be used as a template for creating views in this project.  It uses proper
 * namespacing to allow access to intanciate from the console or the code itself (which is not recommended
 * since we use RequireJS)
 */

define([
	'core',
	'text!html/tpl{{#camelize}}{{name}}{{/camelize}}.html'
], function (core, template) {

	mv.views.{{#camelize}}{{name}}{{/camelize}}View = Backbone.View.extend({

		el: '',

		template: _.template(template),

		initialize: function () {},

		render: function () {},

		events: {}

	});

	return mv.views.{{#camelize}}{{name}}{{/camelize}}View;
});
